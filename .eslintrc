{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true,
  },
  "rules": {
    "no-param-reassign": [2, { "props": false }],
    "no-mixed-operators": [0, {}],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": [0, {}],
    "react/jsx-no-bind": [1, {}],
  }
}
